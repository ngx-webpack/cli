{
  "name": "@ngx-webpack/cli",
  "version": "0.0.12",
  "description": "Customizable Webpack config runner like Angular CLI",
  "main": "index.js",
  "scripts": {
    "dll": "NODE_ENV=install node ./bin/ng-install",
    "start": "node ./bin/ng-start.js --aot",
    "start-aot": "AOT=true webpack-dev-server",
    "build": "node ./bin/ng-build.js --aot",
    "deploy": "node ./bin/ng-deploy.js",
    "lint": "node ./bin/ng-lint.js",
    "test": "node ./bin/ng-test.js",
    "test-watch": "node ./bin/ng-test.js --watch",
    "deploy-npm": "np --skip-cleanup",
    "start-prod": "http-server ./dist -p 3000"
  },
  "bin": {
    "ngx-install": "./bin/ng-install.js",
    "ngx-start": "./bin/ng-start.js",
    "ngx-build": "./bin/ng-build.js",
    "ngx-deploy": "./bin/ng-deploy.js",
    "ngx-test": "./bin/ng-test.js",
    "ngx-lint": "./bin/ng-lint.js"
  },
  "dependencies": {
    "@ngx-webpack/core": "0.0.11",
    "@ngx-webpack/karma": "0.0.4",
    "@ngx-webpack/tslint-config": "0.0.1",
    "gh-pages": "~1.0.0",
    "rimraf": "~2.6.1",
    "webpack-dev-server": "~2.7.1"
  },
  "devDependencies": {
    "@angular/animations": "4.3.3",
    "@angular/common": "4.3.3",
    "@angular/compiler": "4.3.3",
    "@angular/compiler-cli": "4.3.3",
    "@angular/core": "4.3.3",
    "@angular/forms": "4.3.3",
    "@angular/http": "4.3.3",
    "@angular/platform-browser": "4.3.3",
    "@angular/platform-browser-dynamic": "4.3.3",
    "@angular/platform-server": "4.3.3",
    "@angular/router": "4.3.3",
    "@ngx-translate/core": "^7.0.0",
    "codelyzer": "^3.0.0",
    "core-js": "~2.5.0",
    "http-server": "~0.10.0",
    "np": "~2.16.0",
    "rxjs": "~5.4.2",
    "tslint": "^5.0.0",
    "zone.js": "0.8.16"
  },
  "peerDependencies": {
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "author": {
    "name": "blacksonic",
    "email": "soos.gabor86@gmail.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngx-webpack/cli.git"
  },
  "keywords": [
    "angular",
    "webpack",
    "karma",
    "jasmine"
  ],
  "license": "ISC"
}
